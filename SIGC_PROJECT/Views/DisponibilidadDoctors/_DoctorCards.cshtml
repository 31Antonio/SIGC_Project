@model IEnumerable<SIGC_PROJECT.Models.ViewModel.Doctor_DisponibilidadVM>

@foreach (var doctorDisponibilidad in Model)
{
    <div class="col-md-4 doctor-card">
        <div class="card mb-3">
            <div class="card-body">
                <h5 class="card-title">@doctorDisponibilidad.Nombre @doctorDisponibilidad.Apellido</h5>
                <h6 class="card-subtitle mb-2 text-muted">@doctorDisponibilidad.Especialidad</h6>
                <ul class="list-group list-group-flush">
                    @foreach (var disponibilidad in doctorDisponibilidad.Disponibilidades)
                    {
                        <li class="list-group-item">
                            <strong>@disponibilidad.Dia:</strong> @disponibilidad.HoraInicio - @disponibilidad.HoraFin
                        </li>
                    }
                </ul>
            </div>
        </div>
    </div>
}